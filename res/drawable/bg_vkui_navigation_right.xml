<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/vk_ui_navigation_background" />
            <corners android:topRightRadius="8.0dip" android:bottomRightRadius="8.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/vk_ui_navigation_background_pressed" />
            <corners android:topRightRadius="8.0dip" android:bottomRightRadius="8.0dip" />
        </shape>
    </item>
</selector>